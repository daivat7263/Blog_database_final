
<%- include("partials/header"); -%>

<h1>Author</h1>


<%  for(post of user_by_author){ %>

<h1><%=post.title%></h1>
<h5>
  Author: <%=post.author%> 
</h5>
<p>
<%=post.content%>
<a class="btn btn-primary" href="/posts-view/<%=post.id%>">View</a>
<a class="btn btn-success" href="/posts-update/<%=post.id%>">Update</a>
<a class="btn btn-danger" href="/posts-delete/<%=post.id%>">Delete</a>
</p>


<% } %>


<%- include("partials/footer"); -%>
